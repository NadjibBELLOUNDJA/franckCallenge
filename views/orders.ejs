<!DOCTYPE html>

<html>
	<head>
		<meta charset="utf-8">
		<title> Orders List </title>
	</head>
	<body>
		<h1> Orders List </h1>

		<div>
			<ul>
				<% orderslist.forEach(function(order, index) { %>
					<li> 
						<%= order %>
						<a href="/orderslist/finish/<%= index %>"> (Mark as finished) </a>
					</li>
				<% });%>
			</ul>
		</div>
	</body>
</html>